{
  "version": "2.2.0",
  "author": "guhzongren",
  "name": "Powerboard",
  "scripts": {
    "dev": "webpack serve --config build/webpack.dev.js --progress",
    "build": "webpack --config build/webpack.build.js",
    "test": "jest",
    "fix": "prettier -w . && eslint src/**/*.tsx --fix && eslint src/**/*.ts --fix",
    "lint": "prettier -c . && eslint src/**/*.tsx && eslint src/**/*.ts",
    "release": "standard-version --release-as patch",
    "minorRelease": "standard-version --release-as minor",
    "majorRelease": "standard-version --release-as major",
    "cy:run": "cross-env ENV=production cypress run --spec 'cypress/e2e/dashboard.cy.js'",
    "cy:docker": "cross-env ENV=docker cypress run --spec 'cypress/e2e/dashboard.cy.js'",
    "cy:open": "cypress open",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*{.ts,.tsx,.yml}": [
      "lint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "mini-css-extract-plugin": "2.7.2"
  },
  "dependencies": {
    "dayjs": "^1.11.5",
    "graphql": "^16.6.0",
    "graphql-request": "4.3.0",
    "lodash": "^4.17.21",
    "query-string": "^7.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-grid-layout": "^1.3.4",
    "react-router-dom": "^6.4.1",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "14.0.0",
    "@types/jest": "28.1.8",
    "@types/lodash": "^4.14.186",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-grid-layout": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "autoprefixer": "^10.4.12",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cypress": "10.9.0",
    "cypress-downloadfile": "^1.2.3",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.24.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-unused-imports": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "7.3.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "8.0.3",
    "jest": "28.1.3",
    "lint-staged": "13.1.2",
    "mini-css-extract-plugin": "2.7.2",
    "node-sass": "^8.0.0",
    "postcss": "^8.4.17",
    "postcss-loader": "7.0.2",
    "prettier": "^2.7.1",
    "sass-loader": "13.2.0",
    "standard": "17.0.0",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.1",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "workbox-webpack-plugin": "^6.5.4"
  }
}

{
  "version": "1.0.0",
  "author": "yixi",
  "name": "Powerboard",
  "scripts": {
    "dev": "webpack serve --config build/webpack.dev.js --progress",
    "build": "webpack --config build/webpack.build.js",
    "test": "jest",
    "lint": "lint-staged",
    "codeCheck": "yarn tslint -c ./tslint.json --project ./tsconfig.json",
    "typecheck": "tsc --noEmit",
    "release": "standard-version --release-as patch",
    "minorRelease": "standard-version --release-as minor",
    "majorRelease": "standard-version --release-as major",
    "cy:run": "cross-env ENV=production cypress run --spec 'cypress/integration/dashboard.spec.js'",
    "cy:docker": "cross-env ENV=docker cypress run --spec 'cypress/integration/dashboard.spec.js'",
    "cy:open": "cypress open"
  },
  "lint-staged": {
    "src/**/*{.ts,.tsx}": [
      "yarn tslint -c ./tslint.json --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "cypress": "^8.3.1",
    "dayjs": "^1.8.31",
    "graphql": "^15.3.0",
    "graphql-request": "^3.0.0",
    "lodash": "^4.17.19",
    "query-string": "^7.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-grid-layout": "^1.1.1",
    "react-router-dom": "^5.2.0",
    "swr": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.158",
    "@types/query-string": "^6.3.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-grid-layout": "^1.1.0",
    "autoprefixer": "^10.0.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "cypress-downloadfile": "^1.2.1",
    "cypress-file-upload": "^5.0.8",
    "fork-ts-checker-webpack-plugin": "^6.0.3",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^7.0.1",
    "jest": "^27.0.3",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^2.1.0",
    "node-sass": "^6.0.0",
    "pinst": "^2.1.6",
    "postcss": "^8.2.10",
    "postcss-loader": "^6.1.0",
    "sass-loader": "^12.1.0",
    "standard-version": "^9.1.0",
    "style-loader": "^3.0.0",
    "ts-jest": "^27.0.2",
    "ts-loader": "^9.2.2",
    "tslint": "^6.1.3",
    "tslint-react": "^5.0.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.0",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.0.0",
    "workbox-webpack-plugin": "^6.0.2"
  }
}

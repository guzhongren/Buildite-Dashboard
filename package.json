{
  "version": "2.2.0",
  "author": "guhzongren",
  "name": "Powerboard",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "fix": "prettier -w . && eslint src/**/*.tsx --fix && eslint src/**/*.ts --fix",
    "release": "standard-version --release-as patch",
    "minorRelease": "standard-version --release-as minor",
    "majorRelease": "standard-version --release-as major",
    "cy:run": "cross-env ENV=production cypress run --spec 'cypress/e2e/dashboard.cy.js'",
    "cy:docker": "cross-env ENV=docker cypress run --spec 'cypress/e2e/dashboard.cy.js'",
    "cy:open": "cypress open",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*{.ts,.tsx,.yml}": [
      "lint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "dayjs": "^1.11.5",
    "graphql": "^16.6.0",
    "graphql-request": "6.0.0",
    "lodash": "^4.17.21",
    "query-string": "^8.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-grid-layout": "^1.3.4",
    "swr": "^2.1.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.0",
    "@types/lodash": "^4.14.186",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@types/react-grid-layout": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "cypress": "12.12.0",
    "cypress-downloadfile": "^1.2.3",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.24.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.1.2",
    "postcss": "^8.4.23",
    "prettier": "^2.7.1",
    "sass": "^1.62.1",
    "standard": "17.0.0",
    "standard-version": "^9.5.0",
    "ts-jest": "29.2.5",
    "typescript": "^5.0.0",
    "vite": "^4.3.2",
    "vite-plugin-pwa": "^0.14.7"
  }
}
